def ball(numbers):
    if type(numbers) != list:
        raise TypeError("Incorrect Format")
    for element in numbers:
        if type(element) == int:
            print "Yes!"
            return
ball([2, 3, 4])

ball(4)

def car(nums):
	if type(nums) != list:
		raise TypeError("Incorrect Format")
	Positive_numbers = 0
	For element in nums:
		If element > 0:
			positive _numbers += 1
		Return positive_numbers

car([2, 3, 4, 5, 6])

car(4)

car(7)
